<div class="container-fluid fh5co-network">
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <h3>Messages</h3>
          <ul class="messages">
          <% for(var i=0; i<workouts.length; i++) {%>
            <li class="message">
              Workout Name:<p><%= workouts[i].workOutName %></p>
              Body Part:<p><%= workouts[i].bodyPart %></p>
              <p>Weight(lbs):<span><%= workouts[i].weight %></span></p>
              <p>Reps:<span><%= workouts[i].reps %></span></p>
              <p>Sets:<span><%= workouts[i].sets %></span></p>
              <p>Total Amount of Weight Used:<span><%= workouts[i].weight * workouts[i].reps * workouts[i].sets  %></span><p>
              <p><span><i class="fa fa-trash" aria-hidden="true"></i></span></p>
            </li>
          <% } %>
          </ul>

          <h2>Add a Review</h2>

          <form action="/post/createWorkout" method="POST">
            <div class="thePost">
              <label for="workOutName">Workout Name</label>
              <input type="text" class="post" id="workOutName" name="workOutName" required>
            </div> 
            <div class="thePost">
              <label for="bodyPart">Body Part</label>
              <input type="text" class="post" id="bodyPart" name="bodyPart" required>
            </div> 
            <div class="thePost">
              <label for="weight">Weight (lbs)</label>
              <input type="number" class="post" id="weight" name="weight" required>
            </div> 
            <div class="thePost">
              <label for="reps">Reps</label>
              <input type="number" class="post" id="reps" name="reps" required>
            </div> 
            <div class="thePost">
              <label for="sets">Sets</label>
              <input type="number" class="post" id="sets" name="sets" required>
            </div> 

            <button type="submit">Submit</button>
          </form>
        </div>
      
      </div>
      <div class="col-md-6">
    
      </div>
    </div>
  </div>
